#include <vector>
#include <queue>
#include <limits>
#include <list>
#include <array>
#include <iostream>
#include <cmath>

class RadixHeap {
public:
    RadixHeap(long long nC) : K(static_cast<int>(log2(nC))), buckets(K + 1), range(K + 1), distances(nC, std::numeric_limits<long long>::max()), nC(nC) {}
    
private:

};